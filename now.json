{
	"version": 2,
	"builds": [ { "src": "/api/**/*.js", "use": "@now/node" } ],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "/api/$1"
		},
		{
			"src": "/(.*)",
			"dest": "/api/$1"
		}
	],
	"redirects": [
		{
		  "source": "/",
		  "has": [
			{
			  "type": "header",
			  "key": "user-agent",
			  "value": "Go-http-client/1.1"
			}
		  ],
		  "destination": "https://youtube.com/watch?v=dQw4w9WgXcQ",
		  "permanent": false
		}
	  ]
}
